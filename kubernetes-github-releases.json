{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "regexManagers": [
    {
      "description": "Update GitHub Releases",
      "fileMatch": [
        "\\.yaml$"
      ],
      "matchStrings": [
        "https:\\/\\/github\\.com\\/(?<depName>[^\\/]+\\/[^\\/]+)\\/releases\\/download\\/(?<currentValue>[^\\/]+)"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}
